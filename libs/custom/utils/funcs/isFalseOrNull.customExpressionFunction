<?xml version="1.0" encoding="utf-8"?>
<?data-format version="0.400"?>
<CustomExpressionFunction>
  <Name value="isFalseOrNull" />
  <Script><![CDATA[return arg != null ? !arg : true;]]></Script>
  <ResultType BaseType="Bool" />
  <Arguments>
    <Argument>
      <Name value="arg" />
      <ArgumentType BaseType="Bool" />
    </Argument>
  </Arguments>
</CustomExpressionFunction>