<?xml version="1.0" encoding="utf-8"?>
<?data-format version="0.400"?>
<CustomExpressionFunction>
  <Name value="contains" />
  <Script><![CDATA[if (str)
	return str.indexOf(target) > -1;
else
	return false;]]></Script>
  <ResultType BaseType="Bool" />
  <Arguments>
    <Argument>
      <Name value="str" />
      <ArgumentType BaseType="String" />
    </Argument>
    <Argument>
      <Name value="target" />
      <ArgumentType BaseType="String" />
    </Argument>
  </Arguments>
</CustomExpressionFunction>