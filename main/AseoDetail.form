		form.model.modelStructure.ASEO.Event.Authorization.ResponsiblePersonWantsToBeNotified.setTouched(true)
		form.formState.responseResult = false
		result = form.formState.responseResult
	else
		form.model.modelStructure.ASEO.clear()
			
		var messageString = form.actions.getAuthorisationMessage()
			
		form.actions.showErorrNotificationMessage(messageString, false)
		form.VersionGUID = defaultString()
		form.actions.loadAseoData(form.GUID)
		form.formState.responseResult = true
		result = form.formState.responseResult
	endIf
	
	form.formState.lifeCycle.actionConfirm.actionConfirmPanelVisible = false
	
endIf

if form.formState.responseResult = defaultBool() then
	form.formState.responseResult = false
endIf

form.actions.editDataAfterPosting("ASEOSPAuthorization", form.formState.responseResult)

globals.IsBusy = False]]></Script>
