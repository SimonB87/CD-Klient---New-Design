      <ActionLangBody IsValid="True">
        <Script><![CDATA[form.requests.initReq.ASEO.GUID = decisionGuid
form.requests.initReq.ASEO.Event.GUID = decisionVersionGuid

var resp = apiobjects.ServerKomunikace.postAsync("ASEOSPDataRequestEx", form.requests.initReq.toJson(true))
  
if resp.isError() or (resp.getMessageType() = "IN906A") then
    apiobjects.notificationsCtrl.addError("Data povolení nebyla načtena.")
    form.actions.showErorrNotificationMessage("Data povolení nebyla načtena. ", true)
    
    var errorMsg = resp.getErrorMessage()
    system.consoleError("Response content: " + errorMsg)
    
    form.actions.validErr_prepareServerErr(resp.extractJson("QERR"))
else
	form.actions.showErorrNotificationMessage("Data povolení byla úspěšně načtena. ", false)
	
    var respJson = resp.getJsonString()
    form.model.modelStructure.clear()
    form.model.modelStructure.fromJson(respJson)
    
    if form.model.modelStructure.ASEO.Event.GUID.hasValue() then
		form.VersionGUID = form.model.modelStructure.ASEO.Event.GUID
	endIf

	form.isEditOn = false

endIf
