				form.actions.showErorrNotificationMessage("Došlo k chybě načítání dat.", true)
			endIf
		else
			//apiobjects.notificationsCtrl.addError("Došlo k chybě načítání dat.")
			form.actions.showErorrNotificationMessage("Došlo k chybě načítání dat.", true)
		endIf	
		
	endIf
endIf

form.requests.requestCRS.clear()
form.requests.responseCRS.clear()

globals.IsBusy = False
