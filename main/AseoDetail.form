    <ActionBlock>
      <Name value="sendLifeCycleMessage" />
      <Category value="lifeCyclePostingSetUp" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[form.formState.currentDateTime = getDateTime()

var postingSuccess = defaultBool()

if form.formState.currentDateTime <= form.model.modelStructure.ASEO.Event.ValidityStartDate then

	if form.formState.lifeCycle.actionConfirm.actionName = "Upravit žádost" then
		postingSuccess = form.actions.lifeCycle_applicationModification()
	else if form.formState.lifeCycle.actionConfirm.actionName = "Zamítnout žádost" then
		postingSuccess = form.actions.lifeCycle_applicationRejection()
	else if form.formState.lifeCycle.actionConfirm.actionName = "Upravit zamítnutí" then
		postingSuccess = form.actions.lifeCycle_applicationRejectionModification()
	else if form.formState.lifeCycle.actionConfirm.actionName in ["Vydat povolení", "Obnovit povolení"] then
		postingSuccess = form.actions.lifeCycle_authorisation()
	else if form.formState.lifeCycle.actionConfirm.actionName =	 "Upravit povolení" then
		postingSuccess = form.actions.lifeCycle_authorisationModification()	
	else if form.formState.lifeCycle.actionConfirm.actionName in ["Zrušit povolení", "Zrušit povolení."] then
		postingSuccess = form.actions.lifeCycle_authorizationWithdrawal()
	else if form.formState.lifeCycle.actionConfirm.actionName =	 "Upravit zrušení"	then
		postingSuccess = form.actions.lifeCycle_authorizationWithdrawalModification()	
	else if form.formState.lifeCycle.actionConfirm.actionName =	 "Pozastavit"	then
		postingSuccess = form.actions.lifeCycle_authorizationSuspending()	
	else if form.formState.lifeCycle.actionConfirm.actionName =	 "Upravit pozastavení" then
		postingSuccess = form.actions.lifeCycle_authorizationSuspendingModification()	
	else if form.formState.lifeCycle.actionConfirm.actionName =	 "Zneplatit povolení"	then
		postingSuccess = form.actions.lifeCycle_authorizationCancellation()	
	else if form.formState.lifeCycle.actionConfirm.actionName =	 "Upravit zneplatnění"	then
		postingSuccess = form.actions.lifeCycle_authorizationCancellationModification()	
	else if form.formState.lifeCycle.actionConfirm.actionName =	 "Odstranit budoucí změnu"	then
		postingSuccess = form.actions.lifeCycle_futureRemoval()	
	endIf endIf endIf endIf endIf endIf endIf endIf endIf endIf endIf endIf
	
	if postingSuccess then
		form.isEditOn = false
	else
		form.isEditOn = globals.OverenyUzivatel.hasWritePermission
	endIf
	
	if isTrue(form.formState.validationSucess) then
		form.formState.lifeCycle.showActionButtonPanel = true
	else
		form.formState.lifeCycle.showActionButtonPanel = false
	endIf

else
	var message = "V záložce \"Obecné\" nesmí být údaj \"Počátek platnosti rozhodnutí od\" nastaven na čas v minulosti. Současný čas je: \"" + toString(form.formState.currentDateTime) + "\"" 
	form.actions.showErorrNotificationMessage(message, true)
endIf
]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
