    <ActionBlock>
      <Name value="lifeCycle_authorisation" />
      <Category value="lifeCycleMessage" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[globals.IsBusy = True

form.actions.editDataBeforePosting("ASEOSPAuthorization")

form.actions.validErr_clearPrevious()	

var response = apiobjects.ServerKomunikace.postAsync("ASEOSPAuthorization", form.requests.ASEOmegaMessage.toJson())
var responseResult = defaultBool()

if response.isError() then
	system.console(response.getErrorMessage())
	form.actions.showErorrNotificationMessage("Nastala chyba. Žádosti nemohla být autorizace udělena.", true)
	
	form.actions.validErr_prepareServerErr(response.extractJson("QERR"))
	
	if ( (form.authorizationType = "TIR") and (not form.requests.ASEOmegaMessage.ASEO.Event.SPTIR.TIRHolder.hasValue()) ) then
		form.model.modelStructure.ASEO.Event.SPTIR.setTouched(true)
		form.actions.showErorrNotificationMessage("Doplňte Údaj \"Lhůty\" v záložce \"Autorizace\".", true)
	endIf
	
	if not form.requests.ASEOmegaMessage.ASEO.Event.Authorization.hasValue() then
		form.actions.showErorrNotificationMessage("Vyplňte povinné údaje v záložce \"Autorizace\".", true)
	endIf
	
	if ( form.authorizationType = "ACP" ) and (not form.requests.ASEOmegaMessage.ASEO.Event.Period.hasValue()) then
		form.actions.showErorrNotificationMessage("Doplňte aspoň jednu položku \"Lhůty\" v záložce \"Rozšířené\".", true)
	endIf
	
	// setTouch so the user sees, what si missing from submission
	form.model.modelStructure.ASEO.Event.Authorization.LimitedAuthorizationValidity.setTouched(true)
	form.model.modelStructure.ASEO.Event.Authorization.ResponsiblePersonWantsToBeNotified.setTouched(true)
	responseResult = false
else
	form.model.modelStructure.ASEO.clear()
	form.actions.showErorrNotificationMessage("Žádost byla úspěšně předána k autorizaci.", false)
	//test check
	form.VersionGUID.clear()//test check
	form.actions.loadAseoData(form.GUID)
	responseResult = true
endIf

form.formState.lifeCycle.actionConfirm.actionConfirmPanelVisible = false

form.actions.editDataAfterPosting("ASEOSPAuthorization", responseResult)

globals.IsBusy = False
]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="editDataAfterPosting" />
