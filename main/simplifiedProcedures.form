    <ActionBlock>
      <Name value="handler_dataTable1_OnRowActivate" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[form.VersionGUID = params.data.customsDecisionVersionGUID
form.actions.loadData()

//check if user selected last CD version
if (params.data.orderNumber > 0) and form.CD.customsDecisionAuthorisationData.customsDecisionVersionHistory.hasValue() then
	if params.data.orderNumber  = form.CD.customsDecisionAuthorisationData.customsDecisionVersionHistory.count() then
		globals.uiGlobalState.isLastCDversionSelected = True
	else
		globals.uiGlobalState.isLastCDversionSelected = False
	endif

endif]]></Script>
