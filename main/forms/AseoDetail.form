	if isTrue(form.lifeCycleMode in [enums.decisionsState.authorize, enums.decisionsState.authorizeModify]) then
		pathAuth.Authorization.LimitedAuthorizationValidity = true
		pathAuth.Authorization.AuthorizationValidUntil = toDate("2999-12-30", defaultDate())
	endIf

	if pathAuth.GoodsList.GoodsListType.isEmpty() then
		pathAuth.GoodsList.GoodsListType = false
	endIf

	if pathAuth.Seals.SPSealsSpecTypeAllowed.isEmpty() then
		pathAuth.Seals.SPSealsSpecTypeAllowed = false
	endIf
	
	if pathAuth.SPAx.SPFreeFromBindingItineraryAllowed.isEmpty() then
		pathAuth.SPAx.SPFreeFromBindingItineraryAllowed = false
	endIf
	
	if pathAuth.SPAx.ExportDeclarationDischargeByTDAllowed.isEmpty() then
