          <CollectionValidators>
            <Validator Name="requiredPeriods">
              <ErrorMessage IsValid="True">
                <ExpressionText><![CDATA[form.formState.validations.periodsErrorMessage_ACP_SAR_ACR]]></ExpressionText>
                <ResultType>
                  <BaseType value="String" />
                </ResultType>
              </ErrorMessage>
              <IsValid IsValid="True">
                <ExpressionText><![CDATA[(form.lifeCycleMode in [ enums.decisionsState.authorize, enums.decisionsState.authorizeModify ]) ? 
	(isTrue( (form.authorizationType in ["ACP","SAR","ACR"]) ) ? 
		( ((this.Period.exists(current.hasValue()) = false ) ? false : 
			(this.Period.count() < 1) ? false : true) ) : 
		true ) : 
	true]]></ExpressionText>
                <ResultType>
                  <BaseType value="Bool" />
                </ResultType>
              </IsValid>
            </Validator>
            <Validator Name="SPPeriodAuthType">
              <ErrorMessage IsValid="True">
                <ExpressionText><![CDATA["Pokud má pole \"Povolena možnost ukončení vývozního celního prohlášení propuštěním TCP ve ZJP\" hodnotu \"Ano\", je nutné vyplnění lhůty pro VYSTUP. Pokud má pole hodnotu \"Ne\" nesmí být vyplněna lhůta pro VYSTUP."]]></ExpressionText>
                <ResultType>
                  <BaseType value="String" />
                </ResultType>
              </ErrorMessage>
              <IsValid IsValid="True">
                <ExpressionText><![CDATA[(isTrue(form.lifeCycleMode in [enums.decisionsState.authorize, enums.decisionsState.authorizeModify]) AND
isTrue(form.authorizationType = "ACR")) ? 

	( isTrue(form.model.modelStructure.ASEO.Event.SPAx.ExportDeclarationDischargeByTDAllowed) ? 
		isTrue(this.Period.exists(Current.TypeOfPeriod.Key = "VYSTUP")) :
		isFalse(this.Period.exists(Current.TypeOfPeriod.Key = "VYSTUP"))
	) 
	: 
	true
]]></ExpressionText>
                <ResultType>
                  <BaseType value="Bool" />
                </ResultType>
              </IsValid>
            </Validator>
            <Validator Name="SPPeriodCount">
              <ErrorMessage IsValid="True">
                <ExpressionText><![CDATA["Je nutné vyplnit všechny nabízené lhůty. Výjimka je pouze u povolení ACR, kde je vyplnění lhůty VYSTUP podmíněna specifickým pravidlem."]]></ExpressionText>
                <ResultType>
                  <BaseType value="String" />
                </ResultType>
              </ErrorMessage>
              <IsValid IsValid="True">
                <ExpressionText><![CDATA[(isTrue(form.lifeCycleMode in [enums.decisionsState.authorize, enums.decisionsState.authorizeModify]) AND
isTrue(form.authorizationType = "ACR")) ? 

	( isTrue(form.model.modelStructure.ASEO.Event.SPAx.ExportDeclarationDischargeByTDAllowed) ? 

		( isTrue(this.Period.exists(Current.TypeOfPeriod.Key = "DOKONCENI")) AND
		  isTrue(this.Period.exists(Current.TypeOfPeriod.Key = "POVOLENI")) AND
		  isTrue(this.Period.exists(Current.TypeOfPeriod.Key = "VYSTUP")) )	
		
				:
		
		( isTrue(this.Period.exists(Current.TypeOfPeriod.Key = "DOKONCENI")) AND
		  isTrue(this.Period.exists(Current.TypeOfPeriod.Key = "POVOLENI")) ) 
		 
		 ) 
	: 
	true]]></ExpressionText>
                <ResultType>
                  <BaseType value="Bool" />
                </ResultType>
              </IsValid>
            </Validator>
