      <Name value="loadDataFromRegister" />
      <Category value="lifeCyclePostingSetUp" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[globals.IsBusy = True
form.requests.requestCRS.HodnotaIdentifikatoru = form.model.modelStructure.ASEO.TIN
form.requests.requestCRS.KodTypuIdentifikatoru = "EORI"
form.requests.requestCRS.JenUrcujiciIdentifikator = false
form.requests.requestCRS.ZiskatAdresy = false
form.requests.requestCRS.ZiskatIdentifikatory = true
form.requests.requestCRS.ZiskatNespravneUdaje = false
form.requests.requestCRS.ZiskatZpochybneneUdaje = false
form.requests.requestCRS.ZiskatProvozovny = false
form.requests.requestCRS.ZeZakladnichRegistru = true
form.requests.requestCRS.KodEvidence = "ASEO"
form.requests.requestCRS.KodAgendoveRole = "CR328"
form.requests.requestCRS.KodUtvaru = form.model.modelStructure.ASEO.Event.DecidingCustomsOffice
form.requests.requestCRS.Uzivatel = globals.OverenyUzivatel.userName
form.requests.requestCRS.IP = "172.0.0.1"
form.requests.requestCRS.Duvod = "Vydávání povolení v ASEO"

var response = apiobjects.ServerKomunikace.postAsync("SubjektPodleIdentifikatoruRequestCIC", form.requests.requestCRS.toJson())

if (isTrue(response.isError()) OR isTrue(response.getMessageType() = "IN906A")) then
	system.console(response.getErrorMessage())
	apiobjects.notificationsCtrl.addError("Došlo k chybě při komunikaci s Centrem.")

else
	var jsonData = response.getJsonString()
	form.requests.responseCRS.fromJson(jsonData) 
		
	if form.requests.responseCRS.Subjekty.isEmpty() then
		
		form.requests.responseCRS.fromJson(response.getJsonString())
	
		var warningMessage = "Nepodařilo se najít žádné odpovídající subjekty."
		
		if form.requests.responseCRS.PopisChyby.hasValue() then
			warningMessage = warningMessage + "Kód Chyby: \"" + form.requests.responseCRS.PopisChyby + "\""
		endIf
		
		if form.requests.responseCRS.KodChyby.hasValue() then
			warningMessage = warningMessage + "Popis chyby \"" +form.requests.responseCRS.KodChyby + "\""
		endIf
		
		//apiobjects.notificationsCtrl.addWarning(warningMessage)
		form.actions.showErorrNotificationMessage(warningMessage, true)
	else
		
	var mainCol = form.requests.responseCRS.Subjekty.selectFirst()
	if not mainCol.Subjekt.isEmpty() then
		var innerCol = mainCol.Subjekt.selectFirst()
			if not innerCol.SloucenaAdresa.isEmpty() then
				var targetCol = innerCol.SloucenaAdresa.selectFirst()
					form.model.modelStructure.ASEO.Event.OperatorName = targetCol.PlnyNazev
					form.model.modelStructure.ASEO.Event.OperatorStreetAndNr = targetCol.UliceACislo
					form.model.modelStructure.ASEO.Event.OperatorCity = targetCol.Mesto
					form.model.modelStructure.ASEO.Event.OperatorPostCode = targetCol.PSC
					form.model.modelStructure.ASEO.Event.OperatorCountry = targetCol.Zeme
					//apiobjects.notificationsCtrl.addSuccess("Podařilo se získat data z CRS.")
			else
				//apiobjects.notificationsCtrl.addError("Došlo k chybě načítání dat.")
				form.actions.showErorrNotificationMessage("Došlo k chybě načítání dat.", true)
			endIf
		else
			//apiobjects.notificationsCtrl.addError("Došlo k chybě načítání dat.")
			form.actions.showErorrNotificationMessage("Došlo k chybě načítání dat.", true)
		endIf	
		
	endIf
endIf

form.requests.requestCRS.clear()
form.requests.responseCRS.clear()

globals.IsBusy = False
]]></Script>
      </ActionLangBody>
