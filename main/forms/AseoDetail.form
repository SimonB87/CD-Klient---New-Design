        <Script><![CDATA[var tabs = form.components.tabsMenu
var stateCode = form.formState.lifeCycle.stateCode

tabs.Tab01obecne.Title = "Obecné"
tabs.Tab03celniUrady.Title = "Celní úřady"
tabs.Tab04zbozi.Title = "Zboží"

if form.authorizationType in ["SAR"] then
	tabs.Tab05rozsirene.Title = "Lhůty a zajištění"
else
	tabs.Tab05rozsirene.Title = "Rozšířené"
endIf

var currentAnnex = form.model.modelStructure.ASEO.Event.File.count()
var newAnnex = form.model.sekce5prilohy.newFilesCollection.File.count()
tabs.Tab06prilohy.Title = "Text a přílohy (" + toString(currentAnnex + newAnnex) + ")"

tabs.Tab07prehodnoceni.Title = "Přehodnocení"
tabs.Tab08lhutyAzajisteni.Title = "Lhůty a zajištení"
tabs.Tab09rezimy.Title = "Režimy"

tabs.Tab02lifecycle_authorisation.Title = "Povolení" //"Autorizace"
tabs.Tab10lifecycle_applicationRejection.Title = "Zamítnutí žádosti"

if stateCode in ["FUW","AUW"] then
	tabs.Tab11lifecycle_authorizationWithdrawal.Title = "Zrušení"// Zrušení platnosti
else if stateCode in ["AUC"] then
	tabs.Tab11lifecycle_authorizationWithdrawal.Title = "Zneplatnění"// Zrušení platnosti
endIf endIf

tabs.Tab12lifecycle_authorizationSuspending.Title = "Pozastavení"

tabs.Tab13zavery.Title = "Závěry"
tabs.Tab14odpovedneOsosby.Title = "Odpovědné osoby"

tabs.Tab01obecne.isVisible = true
tabs.Tab07prehodnoceni.isVisible = true
tabs.Tab06prilohy.isVisible = true

if stateCode in ["FUR","AUR"] then

	tabs.Tab10lifecycle_applicationRejection.isVisible = true
	
else if stateCode in ["FUA", "AUA", "AUE", "FCA", "FWA"] then

	tabs.Tab02lifecycle_authorisation.isVisible = true
	
	if form.authorizationType in ["ACP", "SAR", "ACR"] then
		tabs.Tab03celniUrady.isVisible = true
		tabs.Tab05rozsirene.isVisible = true
		tabs.Tab04zbozi.isVisible = true
	endIf
	
	if form.authorizationType in ["ACR"] then
		tabs.Tab13zavery.isVisible = true
		tabs.Tab14odpovedneOsosby.isVisible = true
	endIf
	
	if form.authorizationType in ["SAR"] then
		tabs.Tab09rezimy.isVisible = true
		tabs.Tab04zbozi.isVisible = false
	endIf

endIf endif

var lifeCycleMode_notAuthorize = isTrue((form.lifeCycleMode in [enums.decisionsState.authorize]) = false)

if isTrue(stateCode in ["AUC", "FUW", "AUW"]) AND isTrue(lifeCycleMode_notAuthorize) then 	
	tabs.Tab11lifecycle_authorizationWithdrawal.isVisible = true	
else 
	tabs.Tab11lifecycle_authorizationWithdrawal.isVisible = false
endIf

if isTrue(stateCode in ["AUS"]) AND isTrue(lifeCycleMode_notAuthorize) then 
	tabs.Tab12lifecycle_authorizationSuspending.isVisible = true	
else 
	tabs.Tab12lifecycle_authorizationSuspending.isVisible = false
endIf

if isTrue(stateCode in ["APA", "FUA"]) then 
	tabs.Tab07prehodnoceni.isVisible = false
else
	tabs.Tab07prehodnoceni.isVisible = true
endIf
