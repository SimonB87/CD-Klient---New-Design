    <ActionBlock>
      <Name value="validateSPSealsSpecTypeAllowed" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[if 	isTrue(form.lifeCycleMode in [enums.decisionsState.authorize, enums.decisionsState.authorizeModify]) AND 
	isTrue(form.authorizationType = "ACR")  AND
	isTrue(form.model.modelStructure.ASEO.Event.Seals.SPSealsSpecTypeAllowed) then

	form.apiobjects.ASEOZST.setDateTime(getDateTime())

	// set filter to codelist ASEOZST
	var filter = defaultString()
	if isTrue(form.model.modelStructure.ASEO.Event.CustomsOffices.CustomsOfficeOfRelease.count() = 1) then
		var COfirst = form.model.modelStructure.ASEO.Event.CustomsOffices.CustomsOfficeOfRelease.selectFirst()
		filter = "TIN eq '" + form.model.modelStructure.ASEO.TIN + "' and ResponsibleCustomsOffice eq '" + COfirst.CustomsOfficeReferenceNumber.CELUPLAT_R + "'"
		
	else if isTrue(form.model.modelStructure.ASEO.Event.CustomsOffices.CustomsOfficeOfRelease.count() > 1) then
		filter = "TIN eq '" + form.model.modelStructure.ASEO.TIN + "' and ("
		
		forEach office in form.model.modelStructure.ASEO.Event.CustomsOffices.CustomsOfficeOfRelease do
		
			filter = filter + " ResponsibleCustomsOffice eq '" + office.CustomsOfficeReferenceNumber.CELUPLAT_R + "'"
			if office.index() < form.model.modelStructure.ASEO.Event.CustomsOffices.CustomsOfficeOfRelease.count() then
				filter = filter + " or "
			endIf
		
		endFor
		
		filter = filter + ")"
		
	endIf endIf
	
	form.apiobjects.ASEOZST.setFilter(filter)
	
	var data = form.apiobjects.ASEOZST.getDataAsJson()
	codeListASEOZST.fromJson(data)

	// find if there is a relevant CustomOffice for relewant TIN
	if codeListASEOZST.exists(Current.TIN = form.model.modelStructure.ASEO.TIN) then
		form.model.sekce4rozsirene.validSPSealsSpecTypeAllowed = true
		//apiobjects.notificationsCtrl.addSuccess("Lze povolit \"Povolení použití zvláštního typu celních závěr\".")
	else
		form.model.sekce4rozsirene.validSPSealsSpecTypeAllowed = false
		apiobjects.notificationsCtrl.addWarning("Pro toto povolení není možné povolit \"Povolení použití zvláštního typu celních závěr\".")
	endIf

endIf]]></Script>
