    <ActionBlock>
      <Name value="fillStoreAttachedDocumentsCZ" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[if globals.uiGlobalState.isLastCDversionSelected then

    form.actions.refreshCDgUIDversion()

	if globals.uiGlobalState.GUID.hasValue() then
		
		globals.IsBusy = True
		
		//clear target table with Annex data
		globals.uiGlobalState.annex.addOtherAnnex.annexTableToSelect.attachedDocumentsCZ.clear()
		
		//user choosed in UI message "CustomsDecisionCZAnnexesForAuthorizationRequest" or "CustomsDecisionCZAnnexesForHolderRequest"
		if globals.uiGlobalState.annex.addOtherAnnex.isSelectedAnnexForHolder then
			//fire request "CustomsDecisionCZAnnexesForHolderRequest"
			actions.getAnnexCZdataForHolder(form.CD.customsDecisionAuthorisationData.holderInformation.holder.actorIdentification.eORINumber)
		else
			//fire request "CustomsDecisionCZAnnexesForAuthorizationRequest"
			actions.getAnnexCZdataFromPreviousCDversions()
		endif
		
		globals.IsBusy = False
				
		//alert to user, if there were no annex found
		if globals.uiGlobalState.annex.addOtherAnnex.annexTableToSelect.attachedDocumentsCZ.count() < 1 then
			system.alert("Nebyly nalezeny žádné přílohy!")
		endif
		
	else
		system.console("Není vybrána verze povolení!")
	endif

else
	system.alert("Vyberte současnou verzi povolení!")
endif
]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="linkOtherAnnexToCurrentCD" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[if globals.uiGlobalState.isLastCDversionSelected then
	globals.IsBusy = True
	
	//fill in request message with annex data
	globals.uiGlobalState.annex.addOtherAnnex.linkAnnexToCurrentCD.customsDecisionAuthorisationData.customsDecisionGUID = globals.uiGlobalState.GUID
	if globals.uiGlobalState.CDversionGUID.hasValue() then
		globals.uiGlobalState.annex.addOtherAnnex.linkAnnexToCurrentCD.customsDecisionAuthorisationData.customsDecisionVersionGUID = globals.uiGlobalState.CDversionGUID
	endif
	
	//clear data from last round of sending request
	globals.uiGlobalState.annex.addOtherAnnex.linkAnnexToCurrentCD.customsDecisionAuthorisationData.customsDecisionParticulars.customsDecisionAnnex.attachedDocumentsCZ.clear()
	
	var placeHolderAnnex = globals.uiGlobalState.annex.addOtherAnnex.annexTableToSelect.attachedDocumentsCZ.where(current.isDocumentSelected = True)
	var numberOfAddedAnnex = 0
	
	forEach selectedAnnex in placeHolderAnnex do
		//fill data for request message
				globals.uiGlobalState.annex.addOtherAnnex.linkAnnexToCurrentCD.customsDecisionAuthorisationData.customsDecisionParticulars.customsDecisionAnnex.attachedDocumentsCZ.documentType = selectedAnnex.documentType
		globals.uiGlobalState.annex.addOtherAnnex.linkAnnexToCurrentCD.customsDecisionAuthorisationData.customsDecisionParticulars.customsDecisionAnnex.attachedDocumentsCZ.documentIdentifier = selectedAnnex.documentIdentifier
		globals.uiGlobalState.annex.addOtherAnnex.linkAnnexToCurrentCD.customsDecisionAuthorisationData.customsDecisionParticulars.customsDecisionAnnex.attachedDocumentsCZ.numberOfDocuments = selectedAnnex.numberOfDocuments
		globals.uiGlobalState.annex.addOtherAnnex.linkAnnexToCurrentCD.customsDecisionAuthorisationData.customsDecisionParticulars.customsDecisionAnnex.attachedDocumentsCZ.fileReference = selectedAnnex.fileReference
		//Do the request call to link individual selected CZ annex
		var response = apiobjects.cic.postAsync("CustomsDecisionAnnexLink", globals.uiGlobalState.annex.addOtherAnnex.linkAnnexToCurrentCD.toJson())
		//check server reply
		if response.isError() then
			system.console(response.getErrorMessage())
			system.console("Příloha - Identifikátor dokumentu: [" + ToString(selectedAnnex.documentIdentifier) + "] nemohla být nahrána!")
		else
			//system.alert("Příloha - Identifikátor dokumentu: [" + ToString(selectedCZannex.documentIdentifier) + "] byla úspěšně přidána!")
			numberOfAddedAnnex = numberOfAddedAnnex + 1
			form.actions.setAnnexCaption()
		endif
		
	endFor
	
	//clear data from request message
	globals.uiGlobalState.annex.addOtherAnnex.linkAnnexToCurrentCD.customsDecisionAuthorisationData.customsDecisionParticulars.clear()
	
	//dodělat update seznamu příloh nahraných k poslední verzi povolení
	actions.getNewAnnexData()//get updated data for the annex information
	form.actions.getNewDataToForm()
	globals.uiGlobalState.annex.addOtherAnnex.annexTableToSelect.clear() //clear annex table	
	
	
	globals.IsBusy = False
	
	system.alert("Přidali jste k poslední verzi povolení přílohy v počtu: " + toString(numberOfAddedAnnex))
	
else
	system.alert("Vyberte současnou verzi povolení!")
endif
]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
