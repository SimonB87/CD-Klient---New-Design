    <ActionBlock>
      <Name value="alertIfMessageHasNoData" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[var checkExistAnnexAll = form.CD.customsDecisionAuthorisationData.customsDecisionInformation.customsDecisionAnnex.hasValue()

if checkExistAnnexAll then
		
	var checkExistAnnexCZ = form.CD.customsDecisionAuthorisationData.customsDecisionInformation.customsDecisionAnnex.attachedDocumentsCZ.exists(current.hasValue())
			
	if checkExistAnnexCZ = True then
		var checkExistAnnexCZcount = form.CD.customsDecisionAuthorisationData.customsDecisionInformation.customsDecisionAnnex.attachedDocumentsCZ.count()
			if checkExistAnnexCZcount = 0 then
				system.alert("Zpráva neobsahuje žádná data původem z ČR.")
			endIf
		endIf
			
	else
		system.alert("Zpráva neobsahuje žádná příloh data.")
endIf]]></Script>
