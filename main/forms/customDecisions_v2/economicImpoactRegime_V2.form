    <ActionBlock>
      <Name value="storeNewAnnexCZtoCD" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[//call global action
actions.storeNewAnnexCZ()

globals.IsBusy = True

if globals.uiGlobalState.annex.annexCZuploadSuccess = True then

	var response = apiobjects.cic.postAsync("CustomsDecisionAnnexDetailRequest", globals.uiGlobalState.annex.annexDetail.request.toJson())
	
	if response.isError() then
		system.console(response.getErrorMessage())
	else
		
		//clear old annexCZ data 
		var checkExistence = form.CD.customsDecisionAuthorisationData.customsDecisionInformation.customsDecisionAnnex.hasValue()
		
		if (checkExistence = True) and (form.CD.customsDecisionAuthorisationData.customsDecisionInformation.customsDecisionAnnex.isEmpty() = False) then
		
			var targetCollection = form.CD.customsDecisionAuthorisationData.customsDecisionInformation.customsDecisionAnnex
			targetCollection.clear()		
			
		endif
				
		// fill data with newly uploaded annexCZ data
		form.CD.fromJson(response.getJsonString())
		
		form.actions.alertIfMessageHasNoData()
		
	endif
	
	actions.prepareVariablesForNewAnnexUpload()

endif

form.actions.setAnnexCaption()

globals.IsBusy = False]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
