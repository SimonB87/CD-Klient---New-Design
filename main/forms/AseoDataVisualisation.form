<?xml version="1.0" encoding="utf-8"?>
<?data-format version="0.400"?>
<Form>
  <Name value="AseoDataVisualisation" />
  <Caption value="Aseo data visualisation" />
  <Content>
    <Panel>
      <Name value="pnl_body" />
      <Orientation value="Vertical" />
      <Panel.Height value="#" />
      <Panel.Width value="*" />
      <Children>
        <Panel>
          <Name value="pnl_AllowedCountry" />
          <Orientation value="Horizontal" />
          <Panel.Height value="#" />
          <Panel.Width value="*" />
          <Children>
            <DataTable>
              <Name value="AllowedCountry" />
              <Panel.Height value="#" />
              <Panel.Width value="*" />
              <Bindings>
                <Binding SourceProperty="DataItems" Expression="AllowedCountry" />
              </Bindings>
              <Columns>
                <DataColumn>
                  <Name value="column_ZAKZEME" />
                  <Caption value="Kód země zakázané pro zjednodušený postup" />
                  <Width value="100px" />
                  <Content>
                    <Label>
                      <Name value="label_ZAKZEME" />
                      <Bindings>
                        <Binding SourceProperty="Data" Expression="ZAKZEME" />
                      </Bindings>
                      <CssClass>
                        <CssClassConditional>
                          <ClassName value="{theme}-label" />
                          <IsThemedStyle value="True" />
                        </CssClassConditional>
                      </CssClass>
                    </Label>
                  </Content>
                  <HeaderCssClass>
                    <CssClassConditional>
                      <ClassName value="{theme}-datacolumn" />
                      <IsThemedStyle value="True" />
                    </CssClassConditional>
                  </HeaderCssClass>
                </DataColumn>
                <DataColumn>
                  <Name value="column_POPIS" />
                  <Caption value="Název" />
                  <Width value="300px" />
                  <Content>
                    <Label>
                      <Name value="label_POPIS" />
                      <Bindings>
                        <Binding SourceProperty="Data" Expression="POPIS" />
                      </Bindings>
                      <CssClass>
                        <CssClassConditional>
                          <ClassName value="{theme}-label" />
                          <IsThemedStyle value="True" />
                        </CssClassConditional>
                      </CssClass>
                    </Label>
                  </Content>
                  <HeaderCssClass>
                    <CssClassConditional>
                      <ClassName value="{theme}-datacolumn" />
                      <IsThemedStyle value="True" />
                    </CssClassConditional>
                  </HeaderCssClass>
                </DataColumn>
              </Columns>
              <CssClass>
                <CssClassConditional>
                  <ClassName value="{theme}-datatable" />
                  <IsThemedStyle value="True" />
                </CssClassConditional>
              </CssClass>
            </DataTable>
          </Children>
          <CssClass>
            <CssClassConditional>
              <ClassName value="{theme}-panel" />
              <IsThemedStyle value="True" />
            </CssClassConditional>
          </CssClass>
        </Panel>
        <Panel>
          <Name value="pnl_controll" />
          <Orientation value="Horizontal" />
          <Panel.Height value="#" />
          <Panel.Width value="*" />
          <Children>
            <Button>
              <Name value="btn_close" />
              <Caption value="Zpět" />
              <Panel.Height value="#" />
              <Panel.Width value="100%" />
              <OnClick>
                <ActionEvent ReferencedAction="~handler_btn_close_OnClick" />
              </OnClick>
              <CssClass>
                <CssClassConditional>
                  <ClassName value="{theme}-button" />
                  <IsThemedStyle value="True" />
                </CssClassConditional>
              </CssClass>
            </Button>
          </Children>
          <CssClass>
            <CssClassConditional>
              <ClassName value="{theme}-panel" />
              <IsThemedStyle value="True" />
            </CssClassConditional>
          </CssClass>
        </Panel>
      </Children>
      <CssClass>
        <CssClassConditional>
          <ClassName value="{theme}-panel" />
          <IsThemedStyle value="True" />
        </CssClassConditional>
      </CssClass>
    </Panel>
  </Content>
  <DataContext>
    <DataContext>
      <Children>
        <Attribute>
          <Name value="inputJsonData" />
          <Caption value="Input json data" />
          <DataContextParameter.IsInput value="True" />
          <DeclaredType>
            <AttributeType />
          </DeclaredType>
        </Attribute>
        <Attribute>
          <Name value="inputDataType" />
          <Caption value="Input data type" />
          <DataContextParameter.IsInput value="True" />
          <DeclaredType>
            <AttributeType />
          </DeclaredType>
        </Attribute>
        <Collection>
          <Name value="AllowedCountry" />
          <Caption value="Allowed country" />
          <Children>
            <Attribute>
              <Name value="ZAKZEME" />
              <Caption value="ZAKZEME" />
              <DeclaredType>
                <AttributeType />
              </DeclaredType>
            </Attribute>
            <Attribute>
              <Name value="POPIS" />
              <Caption value="POPIS" />
              <DeclaredType>
                <AttributeType />
              </DeclaredType>
            </Attribute>
          </Children>
        </Collection>
      </Children>
    </DataContext>
  </DataContext>
  <OnCreate>
    <ActionEvent ReferencedAction="~init" />
  </OnCreate>
  <Actions>
    <ActionBlock>
      <Name value="handler_btn_close_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[navigation.close("Close")]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="init" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[if isTrue(form.inputDataType = "ZAKZEME") and form.inputJsonData.hasValue() then

	form.AllowedCountry.fromJson(form.inputJsonData)

endIf]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
  </Actions>
</Form>