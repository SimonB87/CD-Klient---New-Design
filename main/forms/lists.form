    <ActionBlock>
      <Name value="handler_invokeEvent_OnChange" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[var model = form.model
var tabs = form.tabs
var uiState = form.uiState

var event = model.event

event.fromJson(params.data.invokeEvent)

if event.type = "onNodeActivated" then
	var ev = model.onNodeActivated 
	ev.fromJson(event.json)

	var split = actions.js_split(ev.id, "_")
	actions.setStrings(split)
	var mod = actions.getString(1)

	uiState.selectedNode = ev.id
	uiState.selectedModule = mod

	if mod in ["News"] then
		tabs.SelectedTab = tabs.tabNews.idx
	else if mod in ["NewsAdm"] then
		tabs.SelectedTab = tabs.tabAdminNews.idx
	else if mod in ["Place"] then
		tabs.SelectedTab = tabs.tabPlace.idx
	else if mod in ["All"] then
		tabs.SelectedTab = tabs.tabAll.idx
		
		form.apiobjects.listCtrl.setReady(false)
		var p2 = actions.getString(2)
		if p2 in ["EIR"] then
			form.apiobjects.listCtrl.setTextFilter("CustomsDecisionType", "EIR")
		else if p2 in ["LE"] then
			form.apiobjects.listCtrl.setTextFilter("CustomsDecisionType", "LE")
		else if p2 in ["SDE"] then
			form.apiobjects.listCtrl.setTextFilter("CustomsDecisionType", "SDE")
		else
			form.apiobjects.listCtrl.clearVisibleFilters()
		endif endif endif		
		form.apiobjects.listCtrl.setReady(true)
	else
		system.console("Unhandled onNodeActivated id " + ev.id)
	endif endif endif endif

	system.console(model.onNodeActivated.toJson())
else
	system.consoleError("Invalid event type " + event.type)
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="handler_lists_OnCreate" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[globals.IsBusy = true

//url router
//form.actions.setUrlRouter()

var master = globals.forms.master
var lists = globals.forms.lists

// # navigation tree
master.isNavigationTreeVisible = false
ntc.resetFinishedSetup()
master.isNavigationTreeVisible = true

ntc.beginSetup("navTrees::_ntLists", defaultString())

var nodeNews = ntc.addNode("News", "Novinky")
var nodeNewsAdm = ntc.addNode("NewsAdm", "Správa novinek")
var nodeAll = ntc.addNode("All", "Všechna povolení")
var nodeAll_Incomplete = nodeAll.addNode("All_Incomplete", "IncompleteCaption") 
var nodeAll_EIR = nodeAll.addNode("All_EIR", "EIR")
var nodeAll_LE = nodeAll.addNode("All_LE", "LE")
var nodeAll_SDE = nodeAll.addNode("All_SDE", "SDE")
var nodeAudit = ntc.addNode("Audit", "Audity")
var nodeReassessment = ntc.addNode("Reassessment", "Přehodnocení")
var nodePlace = ntc.addNode("Place", "Místa")

ntc.finishSetup()

// TODO: ukladat posledni aktivovanou a vracet se na ni 
ntc.focusNode("News")

var tabs = form.tabs
var uiState = form.uiState

form.apiobjects.listCtrl.setReady(false)

form.actions.refreshIncompleteCounts()

if isTrue(lists.listState = defaultString()) then
	// set defaults
	form.apiobjects.listCtrl.setSorting("CustomsDecisionIssueDate:desc")
	
	if isTrue(form.uiState.newsCount < 1) then
		tabs.SelectedTab = (globals.user.hasWritePermission and (form.uiState.incompleteCounts.last > 0)) ?
			tabs.tabLastIncomplete.idx : tabs.tabAll.idx
	   master.newSession = false
	else
	   if isTrueOrDefault(master.newSession) then
	     tabs.SelectedTab = tabs.tabNews.idx 
	     form.apiobjects.DatasetNoticesFullAdmin.setSorting("Checked:desc")
	   else
	     tabs.SelectedTab = tabs.tabAll.idx
	   endIf
	   master.newSession = false
	endIf
	
	form.actions.tabFilter(false)
	
else
	// load serialized state saved in some global variable
	tabs.SelectedTab = globals.forms.lists.fromTab
	form.actions.tabFilter(false)
	form.apiobjects.listCtrl.loadState(lists.listState)
	
endif

form.apiobjects.listCtrl.setReady(true)

form.apiobjects.DatasetNoticesFullAdmin.setReady(false)
form.apiobjects.DatasetNoticesFullAdmin.clearInvisibleColumnsFilters()
form.apiobjects.DatasetNoticesFullAdmin.clearVisibleFilters()
form.apiobjects.DatasetNoticesFullAdmin.setReady(true)

form.apiobjects.placeCtrl.setReady(false)
form.apiobjects.placeCtrl.setReady(true)

//set up form variables
form.uiState.isSelectedMyCUoffice = False
lists.selectedCdExpired = False
//form.actions.checkIfUserPostedNewAseoSubmission() //needs List changed in later version

globals.IsBusy = false]]></Script>
      </ActionLangBody>
