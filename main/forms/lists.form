    <ActionBlock>
      <Name value="tabFilter" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[globals.IsBusy = true

var tabs = form.tabs

if tabs.tabLastIncomplete.IsSelected then
    var basicIncompleteFilter = "(CustomsDecisionType eq 'OPO' or CustomsDecisionType eq 'IPO' or CustomsDecisionType eq 'TEA' or CustomsDecisionType eq 'CW1' or CustomsDecisionType eq 'CW2' or CustomsDecisionType eq 'CWP' or CustomsDecisionType eq 'EUS' or CustomsDecisionType eq 'CCL') and (EnhancementCompleteness eq '"
    form.apiobjects.listCtrl.setInvisibleODataFilter(basicIncompleteFilter + "LastIncomplete')")
endif
if tabs.tabAll.IsSelected then
    //eq all - "Vše"
    form.apiobjects.listCtrl.clearState()
endif
if tabs.tabNewSub.IsSelected then
    //ASEO "Přijaté žádosti"
    form.apiobjects.listCtrl.setInvisibleODataFilter("(CustomsDecisionStatus eq 'APA' or CustomsDecisionStatus eq 'FUR' or CustomsDecisionStatus eq 'FUA')")
endIf
if tabs.tabRejectedApp.IsSelected then
    //eq 'Annulled - dodat kód stavu - "Zamítnuté žádosti"
    form.apiobjects.listCtrl.setInvisibleODataFilter("(CustomsDecisionStatus eq 'AUR')")
endif
if tabs.tabActive.IsSelected then
    //eq 'Active' - ASEO "Platné"
    form.apiobjects.listCtrl.setInvisibleODataFilter("(CustomsDecisionStatus eq '01' or CustomsDecisionStatus eq 'AUA' or CustomsDecisionStatus eq 'FUW')")
endif
if tabs.tabSuspended.IsSelected then
    //eq 'Suspended' - dodat kód stavu - "Pozastavené"
    form.apiobjects.listCtrl.setInvisibleODataFilter("(CustomsDecisionStatus eq '02' or CustomsDecisionStatus eq 'AUS')")
endif
if tabs.tabRevoked.IsSelected then
    //eq 'Revoked' - dodat kód stavu - "Zrušené"
    form.apiobjects.listCtrl.setInvisibleODataFilter("(CustomsDecisionStatus eq '03' or CustomsDecisionStatus eq 'AUW' or CustomsDecisionStatus eq 'FWA')")
endif
if tabs.tabAnnulled.IsSelected then
    //eq 'Annulled - dodat kód stavu - "Zneplatněné"
    form.apiobjects.listCtrl.setInvisibleODataFilter("(CustomsDecisionStatus eq '04'  or CustomsDecisionStatus eq 'FCA' or CustomsDecisionStatus eq 'AUC')")
endif
if tabs.tabZanikle.IsSelected then
    //ASEO "Zaniklé"
    form.apiobjects.listCtrl.setInvisibleODataFilter("(CustomsDecisionStatus eq 'DEA' or CustomsDecisionStatus eq '99' or CustomsDecisionStatus eq 'AUE')")
endIf
if tabs.tabNews.IsSelected then
    form.actions.newsChangeSeeNew(tabs.SelectedTab)
endIf
if tabs.tabAdminNews.IsSelected then
    form.actions.newsChangeSeeNew(tabs.SelectedTab)    
endIf

if (isFalse(tabs.SelectedTab in [tabs.tabNews.idx, tabs.tabAdminNews.idx] )) then 
	if isTrue(refreshCounts) then
	  form.actions.refreshIncompleteCounts()
	endif
	
	form.apiobjects.listCtrl.setSorting("CustomsDecisionIssueDate:desc")
endIf

globals.forms.lists.fromTab = tabs.SelectedTab

globals.IsBusy = false]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext>
          <Children>
            <Attribute>
              <Name value="refreshCounts" />
              <Caption value="no-caption" />
              <DataContextParameter.IsInput value="True" />
              <DeclaredType>
                <AttributeType>
                  <BaseType value="Bool" />
                </AttributeType>
              </DeclaredType>
            </Attribute>
          </Children>
        </ActionDataContext>
      </DataContext>
    </ActionBlock>
    <ActionBlock>
      <Name value="refreshIncompleteCounts" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[var iCnt = form.uiState.incompleteCounts

var basicUrl = "datasets/DatasetCustomsDecision/$count/?$filter=(CustomsDecisionType eq 'OPO' or CustomsDecisionType eq 'IPO' or CustomsDecisionType eq 'TEA' or CustomsDecisionType eq 'CW1' or CustomsDecisionType eq 'CW2' or CustomsDecisionType eq 'CWP' or CustomsDecisionType eq 'EUS' or CustomsDecisionType eq 'CCL') and (EnhancementCompleteness eq '"

// LastIncomplete
var url = basicUrl + "LastIncomplete')"
var resp = apiobjects.cic.getAsync(url)

var respContent = resp.getJsonString()

if resp.isError() OR isTrue(resp.getMessageType() = "IN906A") then
  iCnt.last = 0
  system.consoleError(resp.getErrorMessage())
  system.consoleError("Response content: " + respContent)
else
  iCnt.last = toInt(respContent, -1)
  packages.navTrees.globals._ntLists.IncompleteCount = iCnt.last 
endif

var checkedUrl = "datasets/DatasetNoticesFull/$count/?$filter=(Checked eq 'Ne')"
var response = apiobjects.cic.getAsync(checkedUrl)
var responseContent = response.getJsonString()

if response.isError() then
	form.uiState.newsCount = 0
else
	form.uiState.newsCount = toInt(responseContent, -1)
endIf

]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
