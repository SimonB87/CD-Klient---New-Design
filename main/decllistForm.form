    <ActionBlock>
      <Name value="switchInvisibleFilterByUserCUorganisation" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[// Switch invisible filter to CDs according to code of User's CU organisations
form.apiobjects.listCtrl.setReady(False)

var basicIncompleteFilter = "(CustomsDecisionType eq 'OPO' or CustomsDecisionType eq 'IPO' or CustomsDecisionType eq 'TEA' or CustomsDecisionType eq 'CW1' or CustomsDecisionType eq 'CW2' or CustomsDecisionType eq 'CWP' or CustomsDecisionType eq 'EUS' or CustomsDecisionType eq 'CCL') and (EnhancementCompleteness eq '"

if form.uiState.isSelectedMyCUoffice = False then
	// See only CDs linked to User's CU organisations
	form.apiobjects.listCtrl.setInvisibleODataFilter(basicIncompleteFilter + "LastIncomplete')")
	
else
	// See all CDs
	var startOfUsersCUorganisationCode = SubString( globals.OverenyUzivatel.CuCode,1,4) 
	
	form.apiobjects.listCtrl.setInvisibleODataFilter("(Link" + startOfUsersCUorganisationCode + " eq true) and" + basicIncompleteFilter + "LastIncomplete')")
	
endIf

form.apiobjects.listCtrl.setReady(True)]]></Script>
