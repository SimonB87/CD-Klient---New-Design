      <ActionLangBody IsValid="True">
        <Script><![CDATA[globals.IsBusy = true

if form.tabFilter.SelectedTab.hasValue() then

	if form.tabFilter.SelectedTab = 1 then
        var basicIncompleteFilter = "(CustomsDecisionType eq 'OPO' or CustomsDecisionType eq 'IPO' or CustomsDecisionType eq 'TEA' or CustomsDecisionType eq 'CW1' or CustomsDecisionType eq 'CW2' or CustomsDecisionType eq 'CWP' or CustomsDecisionType eq 'EUS' or CustomsDecisionType eq 'CCL') and (EnhancementCompleteness eq '"
        form.apiobjects.listCtrl.setInvisibleODataFilter(basicIncompleteFilter + "LastIncomplete')")
    endif
    if form.tabFilter.SelectedTab = 2 then
        //eq all - "Vše"
        form.apiobjects.listCtrl.clearState()
    endif
    if form.tabFilter.SelectedTab = 3 then
        //ASEO "Přijaté žádosti"
        form.apiobjects.listCtrl.setInvisibleODataFilter("(CustomsDecisionStatus eq 'APA' or CustomsDecisionStatus eq 'FUR' or CustomsDecisionStatus eq 'FUA')")
    endIf
    if form.tabFilter.SelectedTab = 4 then
        //eq 'Active' - ASEO "Platné"
        form.apiobjects.listCtrl.setInvisibleODataFilter("(CustomsDecisionStatus eq '01' or CustomsDecisionStatus eq 'AUA' or CustomsDecisionStatus eq 'FUW')")
    endif
    if form.tabFilter.SelectedTab = 5 then
        //eq 'Suspended' - dodat kód stavu - "Pozastavené"
        form.apiobjects.listCtrl.setInvisibleODataFilter("(CustomsDecisionStatus eq '02' or CustomsDecisionStatus eq 'AUS')")
    endif
    if form.tabFilter.SelectedTab = 6 then
        //eq 'Revoked' - dodat kód stavu - "Zrušené"
        form.apiobjects.listCtrl.setInvisibleODataFilter("(CustomsDecisionStatus eq '03' or CustomsDecisionStatus eq 'AUW' or CustomsDecisionStatus eq 'FWA')")
    endif
    if form.tabFilter.SelectedTab = 7 then
        //eq 'Annulled - dodat kód stavu - "Zneplatněné"
        form.apiobjects.listCtrl.setInvisibleODataFilter("(CustomsDecisionStatus eq '04'  or CustomsDecisionStatus eq 'AUR' or CustomsDecisionStatus eq 'FCA' or CustomsDecisionStatus eq 'AUC')")
    endif
    if form.tabFilter.SelectedTab = 8 then
        //ASEO "Zaniklé"
        form.apiobjects.listCtrl.setInvisibleODataFilter("(CustomsDecisionStatus eq 'DEA' or CustomsDecisionStatus eq '99' or CustomsDecisionStatus eq 'AUE')")
    endIf
    if form.tabFilter.SelectedTab = 9 then
        form.actions.newsChangeSeeNew(form.tabFilter.SelectedTab)
    endIf
    if form.tabFilter.SelectedTab = 10 then
        form.actions.newsChangeSeeNew(form.tabFilter.SelectedTab)
        
    endIf

endif

if (not (form.tabFilter.SelectedTab in [8, 9])) then 
	if refreshCounts then
	  form.actions.refreshIncompleteCounts()
	endif
	
	form.apiobjects.listCtrl.setSorting("CustomsDecisionIssueDate:desc")
endIf

globals.IsBusy = false]]></Script>
