    <ActionBlock>
      <Name value="getNewDataToForm" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[//get linked annex data from global structure
if globals.uiGlobalState.annex.annexDetail.response.customsDecisionAuthorisationData.customsDecisionParticulars.customsDecisionAnnex.exists(current.hasValue()) then

	if form.CD.customsDecisionAuthorisationData.customsDecisionParticulars.customsDecisionAnnex.exists(current.hasValue()) then
	
		var sourcePath = globals.uiGlobalState.annex.annexDetail.response.customsDecisionAuthorisationData.customsDecisionParticulars.customsDecisionAnnex
		var targetPath = form.CD.customsDecisionAuthorisationData.customsDecisionParticulars.customsDecisionAnnex
		
		targetPath.fromJson(sourcePath.toJson())
		
	endIf
	
endIf]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
