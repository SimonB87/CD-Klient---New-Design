    <ActionBlock>
      <Name value="handler_button_deleteAnnexCZ_OnClick" />
      <IsPrivate value="True" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[//Get index of item in collection
var getAnnexNumber = params.data.index()
globals.uiGlobalState.annex.annexCZaddNewFile.formVariables.help.selectedDocumentGUID = params.data.documentGUID

//Ask user for event confirmation (delete CZ Annex)
if (globals.uiGlobalState.isLastCDversionSelected = true) then
	
	globals.uiGlobalState.annex.annexCZaddNewFile.formVariables.help.confirmFileDelete = 
	apiobjects.pageAPI.confirm("Chcete skutečně CZ přílohu č. " + toString(getAnnexNumber) + " smazat ?")
	
else
	system.alert("CZ příloha č." + toString(getAnnexNumber) +  " nebyla smazána. Nelze mazat přílohy historických verzí!")
endif

globals.IsBusy = True

if globals.uiGlobalState.annex.annexCZaddNewFile.formVariables.help.confirmFileDelete and globals.uiGlobalState.isLastCDversionSelected then
	actions.deleteSelectedAnnexCZ()
	// Delete selected CZ Annex from collection
	params.data.removeMe()
	
	globals.IsBusy = False
	system.alert("Vybraná CZ příloha byla smazána!")
	globals.uiGlobalState.annex.addOtherAnnex.annexTableToSelect.clear() //clear annex table
	
else
	globals.IsBusy = False
	system.alert("Vybraná CZ příloha nemohla být smazána!")
endif

form.actions.setAnnexTabLabel()]]></Script>
