@font-face
	font-family: "Open Sans Light"
	font-style: normal
	font-weight: 300
	src: local("Segoe UI Light"), local("Open Sans Light"), local("OpenSans-Light"), url(fonts/OpenSans-Light.woff) format("woff")

@font-face
	font-family: "Open Sans"
	font-style: normal
	font-weight: 400
	src: local("Segoe UI"), local("Open Sans"), local("OpenSans"), url(fonts/OpenSans.woff) format("woff")

@font-face
	font-family: "Open Sans Bold"
	font-style: normal
	font-weight: 700
	src: local("Segoe UI Bold"), local("Open Sans Bold"), local("OpenSans-Bold"), url(fonts/OpenSans-Bold.woff) format("woff")
	
	
h1
	font-family: "Segoe UI Light", "Open Sans", "Helvetica Neue Light", "HelveticaNeue-Light", sans-serif
	font-weight: 300
	font-size: 54px
	font-size: 3.375rem
	line-height: 54px
	line-height: 3.375rem
	margin: 0px

h2
	font-family: "Segoe UI Light", "Open Sans", "Helvetica Neue Light", "HelveticaNeue-Light", sans-serif
	font-weight: 300
	font-size: 28px
	font-size: 1.75rem
	line-height: 28px
	line-height: 1.75rem
	letter-spacing: 0.01em
	margin: 0px

h3
	font-family: "Segoe UI Light", "Open Sans", "Helvetica Neue Light", "HelveticaNeue-Light", sans-serif
	font-weight: 300
	font-size: 24px
	font-size: 1.5rem
	letter-spacing: 0.01em
	margin: 0px

.sizeH3
	font-family: "Segoe UI Light", "Open Sans", "Helvetica Neue Light", "HelveticaNeue-Light", sans-serif
	font-weight: 300
	font-size: 24px
	font-size: 1.5rem
	letter-spacing: 0.01em
	margin: 0px

h4
	font-family: "Segoe UI Light", "Open Sans", "Helvetica Neue Light", "HelveticaNeue-Light", sans-serif
	font-weight: 300
	font-size: 18px
	font-size: 1.125rem
	letter-spacing: 0.02em
	margin: 0px

h5
	font-family: "Segoe UI Bold", "Open Sans Bold", "Helvetica Neue Bold", sans-serif
	font-weight: bold
	font-size: 16px
	font-size: 1rem
	letter-spacing: 0.02em
	margin: 0px
	color: rgba(0, 0, 0, 0.6)

h6
	font-family: "Segoe UI", "Open Sans", "Helvetica Neue", sans-serif
	font-weight: normal
	font-size: 12px
	letter-spacing: 0.02em
	margin: 0px

.clickable
	h1,h2,h3,h4,h5,h6
		cursor: pointer

	h2
		text-align center
		line-height 1rem 
