html
	.notification
		width: 100%;
		font-size: .875rem;
		background-color: rgba(255,255,255,0.85);
		background-clip: padding-box;
		margin-bottom: 10px;
		border-radius: .25rem;
		border: 0;
		transition: .6s scale-from-right;

		div.row
			position: relative;
			padding: 0 !important;
			width: 100%;
			margin: 0px !important
			border-radius: .35rem;
			background-position: 15px center;
			background-repeat: no-repeat;
			color: var(--black-Space)
			letter-spacing: 0.5px;

			.growl-icon
				float: left;
				width: 8.33333%;
				position: relative;
				height: 20px;
				margin-top: 9px;
				width: 20px;
				background-size: 100%;
				padding-left: 10px;
				font-size: 0;
				padding-right: 10px;

			.growl-message
				padding-left: 20px;

		&-message	
			width: 90%;
			padding-left: 20%;
			padding-top: 15px
			padding-bottom: 15px

		&-panel
			transition none
			padding: 15px
			position: fixed;
			top: 0;
			right: 0;
			height: 100%;
			width: 20%;
			min-width 300px 
			max-width: 350px;		
			background-color: rgba(255,255,255,.7) !important
			overflow-y: auto;
			z-index: 99997;
			backdrop-filter: blur(5px);
			animation: scale-from-right 0.3s cubic-bezier(0.39, 0.575, 0.565, 1) both
			box-shadow: 0 0.46875rem 2.1875rem rgba(31,10,6,0.03), 0 0.9375rem 1.40625rem rgba(31,10,6,0.03), inset 0 0.25rem 0.53125rem rgba(31,10,6,0.05), inset 0 0.125rem 0.1875rem rgba(31,10,6,0.03)

			.notification-icon
				left: 6% !important;
				position: absolute;
				top: 50%;
				transform: translate(-50%, -50%);

			.notification-icon span:before
				font-size 1.7rem !important 

			.notification-button-close
				border-radius 50% !important
				padding: 5px 0;
				outline none !important
				color: var(--black-Space)

				&:focus
					box-shadow none !important; 

				&:hover .fa
					transform rotate(-90deg)

				.fa
					font-size: 18px !important;
					transition .4s all



			.notifications .notification-button
				 width: 10%;
				outline none

		&-success
			background-color: var(--green) !important

			.growl-icon
				background-image: url("data:image/png;base64,") !important

		&-error
			background: var(--red)

			.growl-icon
				background-image: url("data:image/png;base64,") !important;

		&-info
			background-color: var(--blue) !important; /* Line - Serbia */
		
			.growl-icon
				background-image: url("data:image/png;base64,") !important;

		&-warning
			background-color: var(--orange) !important; /* Line Serbia */
			color: #212529;
			
			.growl-icon
				background-image: url("data:image/png;base64,") !important;

		#toast-container
			position: fixed;
			z-index: 999999;
		
		.growl-button-close .fa
			  color: var(--black-Space)

		.growl-button:focus
			outline: none;

		.growl-button-close:focus
			outline: none;

		&-button
			position: absolute !important;
			right: 16px;
			top: 50%;
			margin-top: -16px;

		button.growl-button-close:focus
			box-shadow: none !important;
			
		&s-status-icon
			position: relative
			left: 0;
			top: 0;

		&-trigger .notification-num[data-notifications-count="0"]
			display: none;
			visibility: hidden;

	.notification-panel
		.notification-panel-header
			display none !important

	div.arc-rdeditor
		.notifications
			.notification-panel-header
				display flex !important
				flex-direction: row
				justify-content: flex-end

			button:not(.notification-trigger)
				i
					padding: 0.5rem

					&::before
						color: var(--black-Charcoal)
					
					&.turned-on
						background-color: #83df59 !important;
						border-radius: 0.35rem
