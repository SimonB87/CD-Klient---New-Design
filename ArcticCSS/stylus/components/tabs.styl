	.tabs
		text-align: center;

	.tabs .tab
		display: inline-block;
		text-align: center;
		font-size: 0.875em;
		width: 18%;
		box-shadow: 0px -2px 2px rgba(0, 0, 0, 0.05);
		border-right: 1px solid #dfdfdf;
		position: relative;
		margin-top: 25px;

		&.collapsed
			display none

		&.tab-selected a::after
			height: 6px;


	.tabs .tab:hover
		transition: none;

	.tabs .tab:hover.last
		border-right: 1px solid transparent;

	.tabs .tab:hover + .tab
		border-left: 1px solid #dfdfdf;

	.tabs .tab a
		position: relative;
		padding: 18px 0;
		text-transform: uppercase;
		letter-spacing: 0.1rem;
		line-height: 16px;

	.tabs .tab a::after
		content: "";
		position: absolute;
		width: 100%;
		left: 0;
		bottom: 100%;
		background-color: var(--red-solid)
		height: 0;
		transition-property: height;
		transition-duration: 200ms;
		border-top-right-radius: 0.35rem;
		border-top-left-radius: 0.35rem;

	.tabs .tab a:hover
		padding-top: 18px;

	.tabs .tab a:hover::after
		height: 6px;


	#open-nav-label,
	#close-nav-label
		display: none;


	#nav-ctrl
		display: none;


