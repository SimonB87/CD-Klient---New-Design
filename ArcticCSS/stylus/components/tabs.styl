div.tab-nav button
	border-bottom-left-radius: 0;
	border-bottom-right-radius: 0;
	margin-bottom: -1px;
	border-bottom: none;

div.tab-nav a.active.button
	border-bottom: 0.175em solid #fff;

.tab-content .tab-pane
	display: none;

.tab-content .tab-pane.active
	display: block;
	

